{
	"name": "daku/nette-guzzle-bluescreen",
	"type": "library",
	"description": "This extension adds a new panel to Tracy Bluescreen for Guzzle. ",
	"keywords": ["nette", "tracy-panel", "guzzle"],

	"license": "MIT",
	"authors": [
		{
			"name": "Daniel Kurečka",
			"email": "danielkurecka@gmail.com"
		}
	],

	"require": {
		"php": ">=7.1.0",
		"nette/di": "^2.4 | ^3.0",
		"guzzlehttp/guzzle": "^6.0 | ^7.0"
	},

	"require-dev": {
		"nette/nette": "^2.4 | ^3.0"
	},

	"autoload": {
		"psr-4": {
			"Daku\\Nette\\Guzzle\\": "src"
		}
	},

	"autoload-dev": {
		"psr-4": {
			"Daku\\Nette\\Guzzle\\Tests\\TestApp\\": "tests/testApp/app"
		}
	}
}
